                    app:layout_constraintTop_toBottomOf="@id/imageB2" />

                <ImageView
                    android:id="@+id/imageC3"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageC4"
                    app:layout_constraintStart_toEndOf="@id/imageC2"
                    app:layout_constraintTop_toBottomOf="@id/imageB2" />

                <ImageView
                    android:id="@+id/imageC4"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageC5"
                    app:layout_constraintStart_toEndOf="@id/imageC3"
                    app:layout_constraintTop_toBottomOf="@id/imageB2" />

                <ImageView
