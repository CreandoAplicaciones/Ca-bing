                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/imageD4"
                    app:layout_constraintTop_toBottomOf="@id/imageC2" />

                <ImageView
                    android:id="@+id/imageF1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageF2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imageD1" />

                <ImageView
                    android:id="@+id/imageF2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageF3"
                    app:layout_constraintStart_toEndOf="@id/imageF1"
                    app:layout_constraintTop_toBottomOf="@id/imageD2" />

                <ImageView
                    android:id="@+id/imageF3"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageF4"
                    app:layout_constraintStart_toEndOf="@id/imageF2"
                    app:layout_constraintTop_toBottomOf="@id/imageD2" />

                <ImageView
                    android:id="@+id/imageF4"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageF5"
                    app:layout_constraintStart_toEndOf="@id/imageF3"
                    app:layout_constraintTop_toBottomOf="@id/imageD2" />

                <ImageView
                    android:id="@+id/imageF5"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
