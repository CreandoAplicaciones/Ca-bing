<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="10dp"
        app:cardCornerRadius="17dp"
        android:layout_marginHorizontal="5dp"
        android:layout_marginVertical="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/outside_line">

                <TextView
                    android:id="@+id/textB"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@color/cards"
                    android:text="B"
                    android:textColor="@color/white"
                    android:textSize="50dp"
                    android:gravity="center"
                    android:fontFamily="@font/robotobold"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/textI"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textI"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@color/cards"
                    android:gravity="center"
                    android:text="I"
                    android:textColor="@color/white"
                    android:textSize="50dp"
                    android:fontFamily="@font/robotobold"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/textN"
                    app:layout_constraintStart_toEndOf="@id/textB"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textN"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@color/cards"
                    android:gravity="center"
                    android:text="N"
                    android:textColor="@color/white"
                    android:textSize="50dp"
                    android:fontFamily="@font/robotobold"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/textG"
                    app:layout_constraintStart_toEndOf="@id/textI"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textG"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@color/cards"
                    android:gravity="center"
                    android:text="G"
                    android:textColor="@color/white"
                    android:textSize="50dp"
                    android:fontFamily="@font/robotobold"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/textO"
                    app:layout_constraintStart_toEndOf="@id/textN"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textO"
                    android:layout_width="0dp"
