                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/imageA4"
                    app:layout_constraintTop_toBottomOf="@id/textO" />

                <ImageView
                    android:id="@+id/imageB1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageB2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imageA1" />

                <ImageView
                    android:id="@+id/imageB2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageB3"
                    app:layout_constraintStart_toEndOf="@id/imageB1"
                    app:layout_constraintTop_toBottomOf="@id/imageA2" />

                <ImageView
                    android:id="@+id/imageB3"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageB4"
                    app:layout_constraintStart_toEndOf="@id/imageB2"
                    app:layout_constraintTop_toBottomOf="@id/imageA2" />

                <ImageView
                    android:id="@+id/imageB4"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageB5"
