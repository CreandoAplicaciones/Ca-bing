                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/textG"
                    app:layout_constraintTop_toTopOf="parent" />


                <ImageView
                    android:id="@+id/imageA1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toStartOf="@id/imageA2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/textB"/>

                <ImageView
