                    android:layout_height="0dp"
                    android:background="@drawable/small_outside_line"
                    android:gravity="center"
                    android:src="@drawable/a90"
                    app:layout_constraintDimensionRatio="6:7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/imageB4"
                    app:layout_constraintTop_toBottomOf="@id/imageA2" />

