<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/selectionPlayFragment">
    <fragment
        android:id="@+id/cardsListFragment"
        android:name="com.cartones.de.bingo.en.casa.gratis.loteria.ui.view.list.CardsListFragment"
        android:label="CardsListFragment"
        tools:layout="@layout/fragment_cards_list">
        <action
            android:id="@+id/action_cardsListFragment_to_cardsBingoFragment"
            app:destination="@id/cardsBingoFragment" />
        </fragment>
    <fragment
        android:id="@+id/cardsBingoFragment"
        android:name="com.cartones.de.bingo.en.casa.gratis.loteria.ui.view.cards.CardsBingoFragment"
        android:label="CardsBingoFragment"
        tools:layout="@layout/fragment_cards_bingo">
    <argument
        android:name="numberCard"
        app:argType="com.cartones.de.bingo.en.casa.gratis.loteria.ui.domains.models.NumberCard" />
    </fragment>

    <fragment
        android:id="@+id/cardsList75Fragment"
        android:name="com.cartones.de.bingo.en.casa.gratis.loteria.ui.view.list75.CardsList75Fragment"
        android:label="CardsList75Fragment"
        tools:layout="@layout/fragment_cards_list_75">
        <action
            android:id="@+id/action_cardsList75Fragment_to_cardsBingo75Fragment"
            app:destination="@id/cardsBingo75Fragment" />
    </fragment>

    <fragment
        android:id="@+id/cardsBingo75Fragment"
        android:name="com.cartones.de.bingo.en.casa.gratis.loteria.ui.view.cards75.CardsBingo75Fragment"
        android:label="CardsBingo75Fragment"
        tools:layout="@layout/fragment_cards_bingo75">
        <argument
            android:name="numberCard75"
            app:argType="com.cartones.de.bingo.en.casa.gratis.loteria.ui.domains.models.NumberCard75" />
    </fragment>
    <fragment
        android:id="@+id/selectionPlayFragment"
        android:name="com.cartones.de.bingo.en.casa.gratis.loteria.ui.view.selectionplay.SelectionPlayFragment"
        android:label="SelectionPlayFragment"
        tools:layout="@layout/fragment_selection_play">
        <action
            android:id="@+id/action_selectionPlayFragment_to_cardsListFragment"
            app:destination="@id/cardsListFragment" />
        <action
            android:id="@+id/action_selectionPlayFragment_to_cardsList75Fragment"
            app:destination="@id/cardsList75Fragment" />
    </fragment>

</navigation>